{
    "_id": "biobb_io",
    "desc": "Biobb_io is the Biobb module collection to fetch data to be consumed by the rest of the Biobb building blocks.",
    "github": "https://github.com/bioexcel/biobb_io",
    "readthedocs": "https://biobb-io.readthedocs.io/en/latest/",
    "conda": "https://anaconda.org/bioconda/biobb_io",
    "docker": "https://quay.io/biocontainers/biobb_io:5.0.1--pyhdfd78af_0",
    "singularity": "https://depot.galaxyproject.org/singularity/biobb_io:5.0.1--pyhdfd78af_0",
    "version": "5.0.1",
    "rest": true,
    "tools": [
        {
            "block": "Ligand",
            "tool": "API Call",
            "desc": "This class is a wrapper for downloading a PDB ligand from the Protein Data Bank.",
            "exec": "ligand",
            "docs": "https://biobb-io.readthedocs.io/en/latest/api.html#module-api.ligand",
            "rest": true
        },
        {
            "block": "Pdb",
            "tool": "API Call",
            "desc": "This class is a wrapper for downloading a PDB structure from the Protein Data Bank.",
            "exec": "pdb",
            "docs": "https://biobb-io.readthedocs.io/en/latest/api.html#module-api.pdb",
            "rest": true
        },
        {
            "block": "AlphaFold",
            "tool": "API Call",
            "desc": "This class is a wrapper for downloading a PDB structure from the AlphaFold Protein Structure Database.",
            "exec": "alphafold",
            "docs": "https://biobb-io.readthedocs.io/en/latest/api.html#module-api.alphafold",
            "rest": false
        },
        {
            "block": "PdbVariants",
            "tool": "API Call",
            "desc": "This class creates a text file containing a list of all the variants mapped to a PDB code from the corresponding UNIPROT entries.",
            "exec": "pdb_variants",
            "docs": "https://biobb-io.readthedocs.io/en/latest/api.html#module-api.pdb_variants",
            "rest": true
        },
        {
            "block": "PdbClusterZip",
            "tool": "API Call",
            "desc": "This class is a wrapper for downloading a PDB cluster from the Protein Data Bank.",
            "exec": "pdb_cluster_zip",
            "docs": "https://biobb-io.readthedocs.io/en/latest/api.html#module-api.pdb_cluster_zip",
            "rest": true
        },
        {
            "block": "MemProtMDSimList",
            "tool": "API Call",
            "desc": "This class is a wrapper of the MemProtMD to get all available membrane-protein systems from its REST API.",
            "exec": "memprotmd_sim_list",
            "docs": "https://biobb-io.readthedocs.io/en/latest/api.html#module-api.memprotmd_sim_list",
            "rest": true
        },
        {
            "block": "MemProtMDSimSearch",
            "tool": "API Call",
            "desc": "This class is a wrapper of the MemProtMD to perform advanced searches in the MemProtMD DB using its REST API.",
            "exec": "memprotmd_sim_search",
            "docs": "https://biobb-io.readthedocs.io/en/latest/api.html#module-api.memprotmd_sim_search",
            "rest": true
        },
        {
            "block": "MemProtMDSim",
            "tool": "API Call",
            "desc": "This class is a wrapper of the MemProtMD to download a simulation using its REST API.",
            "exec": "memprotmd_sim",
            "docs": "https://biobb-io.readthedocs.io/en/latest/api.html#module-api.memprotmd_sim",
            "rest": true
        },
        {
            "block": "ApiBindingSite",
            "tool": "API Call",
            "desc": "This class is a wrapper for the PDBe REST API Binding Sites endpoint.",
            "exec": "api_binding_site",
            "docs": "https://biobb-io.readthedocs.io/en/latest/api.html#module-api.api_binding_site",
            "rest": true
        },
        {
            "block": "CanonicalFasta",
            "tool": "API Call",
            "desc": "This class is a wrapper for downloading a FASTA structure from the Protein Data Bank.",
            "exec": "canonical_fasta",
            "docs": "https://biobb-io.readthedocs.io/en/latest/api.html#module-api.canonical_fasta",
            "rest": true
        },
        {
            "block": "Mmcif",
            "tool": "API Call",
            "desc": "This class is a wrapper for downloading a MMCIF structure from the Protein Data Bank.",
            "exec": "mmcif",
            "docs": "https://biobb-io.readthedocs.io/en/latest/api.html#module-api.mmcif",
            "rest": true
        },
        {
            "block": "IdealSdf",
            "tool": "API Call",
            "desc": "This class is a wrapper for downloading an ideal SDF ligand from the Protein Data Bank.",
            "exec": "ideal_sdf",
            "docs": "https://biobb-io.readthedocs.io/en/latest/api.html#module-api.ideal_sdf",
            "rest": true
        },
        {
            "block": "StructureInfo",
            "tool": "API Call",
            "desc": "This class is a wrapper for getting all the available information of a structure from the Protein Data Bank.",
            "exec": "structure_info",
            "docs": "https://biobb-io.readthedocs.io/en/latest/api.html#module-api.structure_info",
            "rest": true
        },
        {
            "block": "MDDB",
            "tool": "API Call",
            "desc": "This class is a wrapper for downloading a trajectory / topology pair from the MDDB Database.",
            "exec": "mddb",
            "docs": "https://biobb-io.readthedocs.io/en/latest/api.html#module-api.mddb",
            "rest": true
        }
    ],
    "dep_pypi": [
        "install_requires=['biobb_common==5.0.0']",
        "python_requires='>=3.9'"
    ],
    "dep_conda": [
        "python >=3.9",
        "biobb_common ==5.0.0"
    ],
    "keywords": [
        "io",
        "mmb",
        "pdb",
        "rcsb",
        "ebi",
        "MemProtMD",
        "sdf",
        "mmcif",
        "fasta",
        "alphafold",
        "mddb"
    ]
}