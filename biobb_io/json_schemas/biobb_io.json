{
    "_id": "biobb_io",
    "desc": "Biobb_io is the Biobb module collection to fetch data to be consumed by the rest of the Biobb building blocks.",
    "github": "https://github.com/bioexcel/biobb_io",
    "readthedocs": "https://biobb-io.readthedocs.io/en/latest/",
    "conda": "https://anaconda.org/bioconda/biobb_io",
    "docker": "https://quay.io/repository/biocontainers/biobb_io",
    "singularity": "https://www.singularity-hub.org/collections/2411/usage",
    "version": "3.0.1",
    "tools" : [
        {
            "block" : "Drugbank", 
            "tool" : "API Call", 
            "desc" : "This class is a wrapper for the Drugbank REST API.",
            "exec" : "drugbank",
            "docs": "https://biobb-io.readthedocs.io/en/latest/api.html#api-drugbank-module"
        },
        {
            "block" : "Ligand", 
            "tool" : "API Call", 
            "desc" : "This class is a wrapper for downloading a PDB ligand from the Protein Data Bank.",
            "exec" : "ligand",
            "docs": "https://biobb-io.readthedocs.io/en/latest/api.html#module-api.ligand"
        },
        {
            "block" : "Pdb", 
            "tool" : "API Call", 
            "desc" : "This class is a wrapper for downloading a PDB structure from the Protein Data Bank.",
            "exec" : "pdb",
            "docs": "https://biobb-io.readthedocs.io/en/latest/api.html#module-api.pdb"
        }, 
        {
            "block" : "PdbVariants", 
            "tool" : "API Call", 
            "desc" : "This class creates a text file containing a list of all the variants mapped to a PDB code from the corresponding UNIPROT entries.",
            "exec" : "pdb_variants",
            "docs": "https://biobb-io.readthedocs.io/en/latest/api.html#module-api.pdb_variants"
        }, 
        {
            "block" : "PdbClusterZip", 
            "tool" : "API Call", 
            "desc" : "This class is a wrapper for downloading a PDB cluster from the Protein Data Bank.",
            "exec" : "pdb_cluster_zip",
            "docs": "https://biobb-io.readthedocs.io/en/latest/api.html#module-api.pdb_cluster_zip"
        }
    ],
    "dep_pypi" : [
        "install_requires=['biobb_common==3.0.1', 'requests ==2.23.0']", 
        "python_requires='==3.7.*'"
    ], 
    "dep_conda" : [
        "python ==3.7.*", 
        "biobb_common ==3.0.1", 
        "requests ==2.23.0"
    ],
    "keywords" : [
        "io", 
        "mmb", 
        "pdb",
        "rcsb",
        "ebi",
        "MemProtMD"
    ]
}